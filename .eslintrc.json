{
    "plugins": [
        "react"
     ],
    "parser": "babel-eslint", 
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "globals": {
        "localStorage": true,
        "document":true,
        "module":true,
        "process":true,
        "console":true,
        "performance":true
    },
    "rules": {
        "react/display-name": [0, { "ignoreTranspilerName": true }]
    }
}